                  "max": "100",
                  "multiplier": 0.01
                },
                {
                  "type": "number",
                  "label": "Step 2",
                  "min": "0",
                  "max": "100",
                  "multiplier": 0.01
                },
                {
                  "type": "number",
                  "label": "Step 3",
                  "min": "0",
                  "max": "100",
                  "multiplier": 0.01
                },
                {
                  "type": "number",
                  "label": "Step 4",
