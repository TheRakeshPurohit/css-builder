          { "name": "From parent", "value": "inherit" },
          { "name": "No fille mode", "value": "none" },
          { "name": "From last keyframe", "value": "forwards" },
          { "name": "From first keyframe", "value": "backwards" },
          {
            "name": "Both from first and last",
            "value": "both"
